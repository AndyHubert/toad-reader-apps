    "native-base": "2.3.10",
    "node-libs-browser": "2.1.0",
    "react": "16.2.0",
    "react-intl": "2.4.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-25.0.0.tar.gz",
    "react-native-progress": "3.4.0",
    "react-navigation": "1.0.0-beta.27",
    "react-redux": "5.0.6",
    "reduce-reducers": "0.1.2",
    "redux": "3.7.2",
    "redux-persist": "4.10.1",
    "xml2js": "0.4.19"
