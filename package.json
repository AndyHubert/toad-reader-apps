{
  "name": "toad-reader-apps",
  "version": "1.0.0",
  "description": "Toad Reader Apps",
  "author": "Andy Hubert",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo start --android",
    "change-tenant": "./scripts/changeTenant.sh",
    "confirm": "read -p 'Are you sure? ' -n 1 -r && echo '\n' && [[ $REPLY =~ ^[Yy]$ ]]",
    "ios": "expo start --ios",
    "push-to-production": "(npm run confirm -s && expo publish --release-channel production) || true",
    "setup-translation": "node ./scripts/makeTranslationFiles.js",
    "start": "expo start"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx-source": "7.2.0",
    "@expo/vector-icons": "8.1.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-preset-expo": "5.0.0",
    "expo": "31.0.6",
    "find-in-files": "^0.5.0",
    "jszip": "3.1.5",
    "native-base": "2.10.0",
    "node-libs-browser": "2.1.0",
    "react": "16.5.0",
    "react-intl": "2.4.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.1.tar.gz",
    "react-native-progress": "3.4.0",
    "react-navigation": "1.5.2",
    "react-redux": "5.0.6",
    "reduce-reducers": "0.1.2",
    "redux": "3.7.2",
    "redux-devtools": "^3.4.0",
    "redux-persist": "4.10.1",
    "stream": "0.0.2",
    "timers": "0.1.1",
    "xml2js": "0.4.19"
  }
}
