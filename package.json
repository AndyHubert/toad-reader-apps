{
  "name": "toad-reader-apps",
  "version": "1.0.0",
  "description": "Toad Reader Apps",
  "author": "Andy Hubert",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo start --android",
    "build-android-beta": "(npm run remind -s && expo build:android --release-channel beta) || true",
    "build-android-production": "(npm run remind -s && expo build:android --release-channel production) || true",
    "build-ios-beta": "(npm run remind -s && expo build:ios --release-channel beta) || true",
    "build-ios-production": "(npm run remind -s && expo build:ios --release-channel production) || true",
    "change-tenant": "./scripts/changeTenant.sh",
    "confirm": "read -p 'Are you sure? (y/N) ' -n 1 -r && echo '\n' && [[ $REPLY =~ ^[Yy]$ ]]",
    "ios": "expo start --ios",
    "list-versions": "node ./scripts/listVersions.js",
    "push-apps-to-production": "(node ./scripts/echoActionToPerform.js --action push-apps-to-production && npm run confirm -s && expo publish --release-channel production) || true",
    "push-to-staging": "(node ./scripts/echoActionToPerform.js --action push-to-staging && npm run confirm -s && npm run push-web-to-staging && expo publish --release-channel staging) || true",
    "push-web-staging-to-production": "(node ./scripts/echoActionToPerform.js --action push-web-staging-to-production && npm run confirm -s && node ./scripts/pushWebStagingToProduction.js) || true",
    "push-web-to-staging": "expo build:web && cp -r web-build-overrides/* web-build && node ./scripts/pushWebToStaging.js",
    "remind": "read -p 'Did you update the version number and rerun change-tenant? ' -n 1 -r && echo '\n' && [[ $REPLY =~ ^[Yy]$ ]]",
    "rollback-web": "node ./scripts/rollBackWeb.js",
    "rollback-apps": "echo 'See the expo documentation:\nhttps://docs.expo.io/versions/latest/distribution/advanced-release-channels/\n'",
    "translate": "node node_modules/inline-i18n/scripts/makeTranslationFiles.js --translations-dir='./src/utils/translations' --translation-file-exclude-regex='current.json' && npm run translate-convert-json-to-csv",
    "translate-convert-csv-to-json": "node node_modules/inline-i18n/scripts/convertTranslationCSVtoJSON.js --translations-dir='./src/utils/translations'",
    "translate-convert-json-to-csv": "node node_modules/inline-i18n/scripts/convertTranslationJSONtoCSV.js --translations-dir='./src/utils/translations'",
    "start": "expo start -w"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx-source": "7.2.0",
    "@eva-design/eva": "1.4.0",
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/hooks": "^2.4.3",
    "@react-native-community/netinfo": "4.6.0",
    "@ui-kitten/components": "4.4.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-preset-expo": "^8.0.0",
    "deepmerge": "4.2.2",
    "expo": "^36.0.0",
    "expo-asset": "~8.0.0",
    "expo-av": "~8.0.0",
    "expo-device": "~2.0.0",
    "expo-document-picker": "~8.0.0",
    "find-in-files": "^0.5.0",
    "inline-i18n": "github:educational-resources-and-services/inline-i18n#d44421bfd684ceddf38e13c7f437ada9fa98432b",
    "jszip": "3.2.2",
    "node-libs-browser": "2.2.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.1.tar.gz",
    "react-native-circular-progress": "1.3.6",
    "react-native-flip-editor": "github:educational-resources-and-services/react-native-flip-editor#cb5dc3d571ec7175e615ccf240e252bc292fd232",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-native-reanimated": "~1.4.0",
    "react-native-safe-area-context": "0.6.0",
    "react-native-simple-side-menu": "github:educational-resources-and-services/react-native-simple-side-menu#22d795ae9000f829365b347c1d8aa82b48c61a30",
    "react-native-svg": "9.13.3",
    "react-native-table-component": "1.2.1",
    "react-native-view-shot": "3.1.2",
    "react-native-web": "^0.11.7",
    "react-native-webview": "7.4.3",
    "react-redux": "7.1.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-router-native": "5.1.2",
    "react-timer-mixin": "0.13.4",
    "react-use": "^13.24.0",
    "reduce-reducers": "1.0.4",
    "redux": "4.0.4",
    "redux-devtools": "3.5.0",
    "redux-persist": "6.0.0",
    "stream": "0.0.2",
    "timers": "0.1.1",
    "uuid": "3.4.0",
    "xml2js": "0.4.23"
  }
}
