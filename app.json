{
  "expo": {
    "name": "BibleMesh eReader",
    "description": "Toad Reader for BibleMesh",
    "slug": "biblemesh-ereader",
    "privacy": "unlisted",
    "sdkVersion": "23.0.0",
    "version": "1.0.0",
    "orientation": "default",
    "primaryColor": "#dfe7f1",
    "icon": "./assets/icons/biblemesh_logo.png",
    "notification": {
      "icon": "./assets/icons/biblemesh_sm_bw.png",
      "color": "#2c5b8e"
    },
    "loading": {
      "icon": "./assets/icons/biblemesh_logo.png",
      "hideExponentText": true
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff"
    },
    "scheme": "biblemesh",
    "packagerOpts": {
      "assetExts": ["ttf", "mp4"],
      "config": "rn-cli.config.js"
    },
    "android": {
      "package": "com.biblemesh.read"
    },
    "ios": {
      "bundleIdentifier": "com.biblemesh.read",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "associatedDomains": [
        "https://read.biblemesh.com"
      ]
    }
  }
}
