{
  "expo": {
    "name": "BibleMesh eReader",
    "description": "Toad Reader for BibleMesh",
    "slug": "biblemesh-ereader",
    "privacy": "unlisted",
    "sdkVersion": "23.0.0",
    "version": "1.0.0",
    "orientation": "default",
    "primaryColor": "#dfe7f1",
    "icon": "./assets/icons/biblemesh_logo.png",
    "notification": {
      "icon": "./assets/icons/biblemesh_sm_bw.png",
      "color": "#2c5b8e"
    },
    "loading": {
      "icon": "./assets/icons/biblemesh_logo.png",
      "hideExponentText": true
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff"
    },
    "scheme": "biblemesh",
    "packagerOpts": {
      "assetExts": ["ttf", "mp4"],
      "config": "rn-cli.config.js"
    },
    "android": {
      "package": "com.biblemesh.read"
    },
    "ios": {
      "bundleIdentifier": "com.biblemesh.read",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "associatedDomains": [
        "https://read.biblemesh.com"
      ]
    },
    "extra": {
      "ANDROID_TOOLBAR_COLOR": "#4075ae",
      "ANDROID_STATUS_BAR_COLOR": "#2c5b8e",
      "LIBRARY_MAXIMUM_BOOK_COVER_WIDTH": 100,
      "PAGE_LIST_MAXIMUM_PAGE_SIZE": 150,
      "PAGE_LIST_HEADER_ROW_HEIGHT": 40,
      "PAGES_ROW_EXTRA_VERTICAL_SPACE": 10,
      "PROGRESS_BAR_SIDE_SPACING": 20,
      "INITIAL_PAGE_CAPTURE_LOAD_TIMEOUT": 2000,
      "REMOVE_ICON_COLOR": "#fa213b",
      "SPINNER_COLOR": "#4075ae"
    }
  }
}
